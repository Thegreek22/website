<%= stylesheet_link_tag "login/login", media: "all" %>
<!--web-fonts-->
<link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
<!--/web-fonts-->
</head>
<body>
<!--User-Login-->
<h1>User Login</h1>
<div class="avtar">
	<img src="images/avtar.png" />
</div>
	<div class="login-form">
		<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
			<div class="form-text">
				<%= f.email_field :email, autofocus: true, :class =>"text", :value =>"USERNAME", :onfocus =>"this.value = '';", :onblur=>"if (this.value == '') {this.value = 'USERNAME';}"%>
				<%= f.password_field :password, autocomplete: "off", :value=>"Password", :onfocus=>"this.value = '';", :onblur=>"if (this.value == '') {this.value = 'Password';}" %>
			</div>
				<%= f.submit "Ingresar" %>
		<%end%>
	</div>
<!--/User-Login-->
<!--start-copyright-->
<div class="copy-right">
	<p>Template by <a href="http://w3layouts.com">w3layouts</a></p> 
</div>
